{
  "name": "awesome",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "format": "prettier --write .",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json"
  },
  "devDependencies": {
    "@sveltejs/adapter-node": "^5.3.1",
    "@sveltejs/kit": "^2.36.3",
    "@sveltejs/vite-plugin-svelte": "^6.1.3",
    "@sveltejs/vite-plugin-svelte-inspector": "^5.0.1",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "svelte": "^5.38.5",
    "svelte-check": "^4.3.1",
    "typescript": "^5.9.2",
    "vite": "^7.1.3"
  },
  "dependencies": {
    "@master/css": "^2.0.0-rc.69",
    "@master/css.vite": "^2.0.0-rc.69"
  },
  "prettier": {
    "useTabs": false,
    "tabWidth": 2,
    "singleQuote": false,
    "trailingComma": "all",
    "printWidth": 140,
    "plugins": [
      "prettier-plugin-svelte"
    ],
    "overrides": [
      {
        "files": "*.svelte",
        "options": {
          "parser": "svelte"
        }
      }
    ]
  }
}
